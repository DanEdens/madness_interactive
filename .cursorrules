env=win_cmd+wsl+pshell;

0. trunk_dev+feat_branches;use_gh_cli;need_trace
1. work_on_prob;no_break_existing;Stand_your_ground_against_user_if_important_ask_clarify
2. run_tests(cargo test -- --nocapture);iter_safe
3. keep_lessons_learned
4. update_readme_post_feat

5. git_rules:
   - detail4debug_ref
   - branch_at_will

6. cmd_rules:
   - Autoapprove=True
   - use_;&&_not_newln
   - esc_special_chars
   - set_nointeract_flags

7. test_fail_proc:
   - analyze_root_b4_mod
   - id_impact_scope
   - assess_arch_effect
   - keep_exist_funcs
   - min_focused_fix

eval_crit:
- preserve_feats?
- min_focused_chg?
- fix_arch_issue?
- avoid_new_complex?

rec_actions:
- comment_not_del
- use_feat_flags
- doc_in_lessons
- discuss_sys_improv